{
  "name": "acsl-astra-c-extension",
  "displayName": "acsl-astra-c-extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MisterZurg/acsl-ansi-iso-c-specification-language"
    },
  "categories": [
    "Programming Languages"
  ],
  "author": {
    "name": "Vladimir Zhuravlev"
    },
  "publisher": "VladimirZHC",
  "contributes": {
    "languages": [{
      "id": "acsl",
      "aliases": [
        "Astra-Acsl-CSL", 
        "acsl"
    ],
      "extensions": [
        ".cpp", 
        ".c"
        ],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [
        {
            "language": "acsl",
            "scopeName": "source.acsl",
            "path": "./syntaxes/acsl.tmLanguage.json",
            "embeddedLanguages": {
                "meta.embedded.block.c": "c",
                "meta.embedded.block.cpp": "cpp"
            }
        },
        {
            "language": "c",
            "scopeName": "source.c",
            "path": "./syntaxes/c.tmLanguage.json"
        },
        {
            "language": "cpp",
            "scopeName": "source.cpp",
            "path": "./syntaxes/cpp.tmLanguage.json"
        }
    ]
  }
}
